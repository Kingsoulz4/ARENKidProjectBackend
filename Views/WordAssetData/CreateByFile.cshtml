@model ProjectBackend.Models.WordAssetData

<h1>Create Word Assets by zip file</h1>

<h4>Model3DData</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form enctype="multipart/form-data" asp-action="CreateByFile" >
            <span>Select your file </span>
            <input type="file" name="fileToUpload" accept=".zip" />

            <br/>
            <span> @Html.Raw(ViewBag.Message)</span>

            <div class="form-group">
                <input type="submit" value="CreateByFile" class="btn btn-primary" />
            </div>
        </form>

    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}